¬ (A ∧ B) ⇔ (¬ A ⋁ ¬ B)

(=>)
  If ¬ (A ∧ B):
    If ¬ (¬ A ⋁ ¬ B):
      If A:
        If B:
          A ∧ B
          ⊥ 
        ¬ B
        ¬ A ⋁ ¬ B
        ⊥ 
      ¬ A
      ¬ A ⋁ ¬ B
      ⊥ 
    ¬ A ⋁ ¬ B
  ¬ (A ∧ B) ⇒ (¬ A ⋁ ¬ B)
(<=)
  If (¬ A ⋁ ¬ B):
    If A ∧ B:
      If ¬ A:
        A
        ⊥ 
      If ¬ B:
        B
        ⊥
      ⊥ 
    ¬ (A ∧ B)
  (¬ A ⋁ ¬ B) ⇒ ¬ (A ∧ B)
